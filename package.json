{
  "name": "react-three-fiber-test",
  "version": "1.0.0",
  "description": "react-three-fiber test",
  "scripts": {
    "build": "rimraf public && yarn format && yarn lint && yarn type-check && gatsby build",
    "check-format": "prettier --list-different \"src/**/*.{ts,tsx,css,html,json,md}\" ",
    "create-types-for-css-modules": "tcm src -c",
    "create-types-for-sass-modules": "tsm src --nameFormat camel",
    "css-watch": "tcm src -c --watch",
    "develop": "gatsby develop -p 8001",
    "eslint-init": "eslint --init",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,html,json,md}\" ",
    "lint": "eslint ./ --ext .js,.ts,.tsx,.jsx --format visualstudio --no-color --max-warnings 10 --report-unused-disable-directives --ignore-pattern 'public/*' --ignore-pattern 'coverage/*' --ignore-pattern 'node_modules/*' ",
    "sass-watch": "tsm src --nameFormat camel --watch",
    "serve": "gatsby serve",
    "start": "yarn develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "tsc-init": "tsc --init",
    "type-check": "tsc"
  },
  "dependencies": {
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "gatsby": "2.15.12",
    "gatsby-plugin-sass": "2.1.14",
    "gatsby-plugin-typescript": "2.1.7",
    "node-sass": "4.12.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-three-fiber": "5.2.1",
    "three": "0.122.0",
    "typescript": "4.0.5"
  },
  "devDependencies": {
    "@types/node": "12.7.4",
    "@types/node-sass": "4.11.0",
    "@typescript-eslint/eslint-plugin": "2.2.0",
    "@typescript-eslint/parser": "2.2.0",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2.0",
    "typed-css-modules": "0.6.2",
    "typed-scss-modules": "0.0.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicklasisraelsson/react-three-fiber-test.git"
  },
  "author": "Nicklas Israelsson",
  "license": "MIT",
  "homepage": "https://github.com/nicklasisraelsson/react-three-fiber-test#readme"
}
